{
  "name": "nestjs-boilerplate",
  "version": "1.0.3",
  "description": "Simple template to get started with Nest JS",
  "author": "Priyam",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "release": "bumpp package.json package-lock.json --commit --push --tag"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.3.0",
    "@nestjs/platform-fastify": "^8.3.0",
    "@ogma/nestjs-module": "^3.1.1",
    "@ogma/platform-fastify": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify": "^3.27.1",
    "fastify-cookie": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.2",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.6",
    "@types/node": "^17.0.17",
    "bumpp": "^7.1.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "run-script-webpack-plugin": "^0.0.11",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-node-externals": "^3.0.0"
  }
}
